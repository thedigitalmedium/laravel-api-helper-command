{
    "name": "thedigitalmedium/api-helper-command",
    "description": "Effortlessly Generate Essential API Files - Building APIs often involves juggling multiple files and configurations. The API Helper Command simplifies this process, automating the generation of key files for seamless API development. (https://thedigitalmedium.com)",
    "keywords": [
        "laravel",
        "api",
        "api-generator"
    ],
    "homepage": "https://github.com/thedigitalmedium/laravel-api-helper-command",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Md Sultanul Arefin",
            "email": "programmer.areifn@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/http": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/container": "^8.0|^9.0|^10.0",
        "illuminate/auth": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "thedigitalmedium/laravel-api-helper": "^1.1",
        "tightenco/duster": "^1.2",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "TheDigitalMedium\\ApiHelperCommand\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheDigitalMedium\\ApiHelperCommand\\ApiHelperCommandServiceProvider",
                "TheDigitalMedium\\ApiHelperCommand\\MacroServiceProvider"
            ]
        }
    },
    "scripts": {
        "fix-style": "./vendor/bin/duster fix",
        "check-code": "./vendor/bin/duster lint"
    }
}
